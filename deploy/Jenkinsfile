pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    sh 'echo "Building Stage"'
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    sh 'echo "Testing Stage"'
                }
            }
        }
        stage('Package') {
            steps {
                script {
                    sh 'echo "Packaging Application"'
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    sh 'echo "Deploying Application"'
                }
            }
        }
        stage('Clean up') {
            steps {
                script {
                    sh 'echo "Cleaning Up Environment"'
                }
            }
        }
    }
}
